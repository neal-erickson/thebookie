{
  "manifest_version": 2,

  "name": "Bookmarkhor",
  "description": "Epic-ly awesome bookmark navigation",
  "version": "0.1",

  "permissions": [
    "tabs",
    "bookmarks"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  }
}