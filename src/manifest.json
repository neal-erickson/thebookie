{
  "manifest_version": 2,

  "name": "The Bookie",
  "description": "Epic-ly awesome bookmark navigation",
  "version": "0.1",

  "permissions": [
    "tabs",
    "bookmarks"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  }
}